<div class = 'navigation_bar_dashboard'> 
    <ul>
        <a href='/employees'><li>ALL QUOTES</li></a>
        <a href='/employee_email_managers'><li>NOTIFICATION MANAGER</li></a>
    </ul>
</div>

<p id="notice"><%= notice %></p>

<h1><%= @company.name %></h1>
<table>
        <tr>
                <td>Campagin Name</td>
                <td>Number of Clicks</td>
                <td>Target Demographics</td>
                <td>Subtotal</td>
        </tr>
        <% @quotes.each do |quote| %>
                <% if quote.company_id == @company.id %>
                <tr>
                        <td><%= quote.location %></td>
                        <td><%= quote.location %></td>
                        <td><%= quote.ttc %></td>
                        <td><%= quote.age %></td>
                        <td><%= quote.weeks_pregnant %></td>
                        <td><%= quote.days_since_signup %></td>
                        <td><%= quote.fertility_condition %></td>
                        <td><%= quote.cycle_stage %></td>
                        <td><%= quote.house_hold_income %></td>
                        <td><%= quote.monthly_acivity %></td>
                        <td><%= quote.budget %></td>
                        <td><%= quote.begin_date %></td>
                        <td><%= quote.begin_date %></td>
                        <td><%= quote.campaign_name %></td>
                        <td><%= quote.ovuline_exclusive %></td>
                        <td><%= quote.native_ad %></td>
                        <td><%= quote.native_article %></td>
                        <td><%= quote.social_ad %></td>


<!--                         <td><%= quote.adtype %></td> -->
                        <td><%= quote.views %></td>

                        <td><%= link_to 'Remove', quote, method: :delete %></td>
                </tr>
                <% end %>
        <% end %>
</table>
<%= link_to 'Edit', edit_company_path(@company) %> |
<%= link_to 'Back', companies_path %>
